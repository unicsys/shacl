PREFIX : <http://www.yourdomain.com/ontology/dido#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?standard ?label
WHERE {
  # Find any subject (?standard) that has the type :Standard.
  ?standard rdf:type :Standard .
  
  # Get its human-readable label to make the results clear.
  OPTIONAL { ?standard rdfs:label ?label . }
}
ORDER BY ?label

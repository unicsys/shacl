                                                                                                                              ]
                                                                                                         ]
                                                                                                       )
                                                                                         ] ,
                                                                                         [ rdf:type owl:Restriction ;
                                                                                           owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000054> ;
                                                                                           owl:allValuesFrom [ owl:intersectionOf ( <https://spec.industrialontologies.org/ontology/core/Core/PlannedProcess>
                                                                                                                                    [ rdf:type owl:Restriction ;
                                                                                                                                      owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000055> ;
                                                                                                                                      owl:someValuesFrom [ owl:intersectionOf ( <http://purl.obolibrary.org/obo/BFO_0000034>
                                                                                                                                                                                [ rdf:type owl:Restriction ;
                                                                                                                                                                                  owl:onProperty <https://spec.industrialontologies.org/ontology/core/Core/functionOf> ;
                                                                                                                                                                                  owl:someValuesFrom <https://spec.industrialontologies.org/ontology/core/Core/MaterialArtifact>
                                                                                                                                                                                ]
                                                                                                                                                                              ) ;
                                                                                                                                                           rdf:type owl:Class

<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/university#"
     xml:base="http://example.org/university"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://example.org/university">
        <rdfs:label>University Ontology</rdfs:label>
        <rdfs:comment>A simple ontology for testing purposes</rdfs:comment>
    </owl:Ontology>
    
    <!-- Classes -->
    <owl:Class rdf:about="http://example.org/university#Person">
        <rdfs:label>Person</rdfs:label>
        <rdfs:comment>A human being</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/university#Student">
        <rdfs:label>Student</rdfs:label>
        <rdfs:comment>A person who studies at a university</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://example.org/university#Person"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/university#Professor">
        <rdfs:label>Professor</rdfs:label>
        <rdfs:comment>A person who teaches at a university</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://example.org/university#Person"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/university#Course">
        <rdfs:label>Course</rdfs:label>
        <rdfs:comment>An academic course</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/university#Department">
        <rdfs:label>Department</rdfs:label>
        <rdfs:comment>An academic department</rdfs:comment>
    </owl:Class>
    
    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="http://example.org/university#enrolledIn">
        <rdfs:label>enrolled in</rdfs:label>
        <rdfs:comment>Relates a student to a course</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Student"/>
        <rdfs:range rdf:resource="http://example.org/university#Course"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/university#teaches">
        <rdfs:label>teaches</rdfs:label>
        <rdfs:comment>Relates a professor to a course</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Professor"/>
        <rdfs:range rdf:resource="http://example.org/university#Course"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/university#belongsTo">
        <rdfs:label>belongs to</rdfs:label>
        <rdfs:comment>Relates a course to a department</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Course"/>
        <rdfs:range rdf:resource="http://example.org/university#Department"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/university#workAt">
        <rdfs:label>works at</rdfs:label>
        <rdfs:comment>Relates a professor to a department</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Professor"/>
        <rdfs:range rdf:resource="http://example.org/university#Department"/>
    </owl:ObjectProperty>
    
    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="http://example.org/university#hasName">
        <rdfs:label>has name</rdfs:label>
        <rdfs:comment>The name of a person</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/university#hasAge">
        <rdfs:label>has age</rdfs:label>
        <rdfs:comment>The age of a person</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/university#hasCredits">
        <rdfs:label>has credits</rdfs:label>
        <rdfs:comment>Number of credits for a course</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Course"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    
    <!-- Axioms -->
    <owl:Class rdf:about="http://example.org/university#Student">
        <owl:disjointWith rdf:resource="http://example.org/university#Professor"/>
    </owl:Class>
    
    <!-- Sample Instances -->
    <Student rdf:about="http://example.org/university#john">
        <hasName>John Smith</hasName>
        <hasAge rdf:datatype="http://www.w3.org/2001/XMLSchema#int">20</hasAge>
        <enrolledIn rdf:resource="http://example.org/university#math101"/>
    </Student>
    
    <Professor rdf:about="http://example.org/university#jane">
        <hasName>Jane Doe</hasName>
        <hasAge rdf:datatype="http://www.w3.org/2001/XMLSchema#int">45</hasAge>
        <teaches rdf:resource="http://example.org/university#math101"/>
        <workAt rdf:resource="http://example.org/university#mathDept"/>
    </Professor>
    
    <Course rdf:about="http://example.org/university#math101">
        <hasName>Introduction to Mathematics</hasName>
        <hasCredits rdf:datatype="http://www.w3.org/2001/XMLSchema#int">3</hasCredits>
        <belongsTo rdf:resource="http://example.org/university#mathDept"/>
    </Course>
    
    <Department rdf:about="http://example.org/university#mathDept">
        <hasName>Mathematics Department</hasName>
    </Department>
    
</rdf:RDF>

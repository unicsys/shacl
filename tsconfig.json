                                                                                                                                                                                          [ rdf:type owl:Restriction ;
                                                                                                                                                                                            owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000177> ;
                                                                                                                                                                                            owl:someValuesFrom <https://spec.industrialontologies.org/ontology/core/Core/PlanSpecification>
                                                                                                                                                                                          ]
                                                                                                                                                                                        )
                                                                                                                                                                          ]
                                                                                                                                                     ]
                                                                                                                                                   ) ;
                                                                                                                                rdf:type owl:Class
                                                                                                                              ]

@prefix : <http://example.org/myontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/BFO_> .
@prefix iao: <http://purl.obolibrary.org/obo/IAO_> .
@prefix ro: <http://purl.obolibrary.org/obo/RO_> .
@prefix org: <http://www.w3.org/ns/org#> . # Assuming W3C ORG ontology for Manufacturer

# Ontology Declaration
<http://example.org/myontology> rdf:type owl:Ontology ;
    owl:imports iao: , ro: , bfo: , org: ; # Import necessary ontologies
    rdfs:comment "Ontology for DID requirements, focusing on Requirement 2 of DI-SESS-81000F." .

# Object Properties
:conforms_to_standard rdf:type owl:ObjectProperty ;
    rdfs:label "conforms to standard" .

:serves_as rdf:type owl:ObjectProperty ;
    rdfs:label "serves as" .

:reflects rdf:type owl:ObjectProperty ;
    rdfs:label "reflects" .

:provides_engineering_data_for rdf:type owl:ObjectProperty ;
    rdfs:label "provides engineering data for" .

:provides_data_permitting rdf:type owl:ObjectProperty ;
    rdfs:label "provides data permitting" .

:concerns_item rdf:type owl:ObjectProperty ;
    rdfs:label "concerns item" .

:duplicates_specified_characteristics_of rdf:type owl:ObjectProperty ;
    rdfs:domain :Item ;
    rdfs:range :Item ;
    rdfs:label "duplicates specified characteristics of" .

:maintains_quality_for_duplication_of rdf:type owl:ObjectProperty ;
    rdfs:domain bfo:0000015 ; # bfo:Process
    rdfs:range :Item ;
    rdfs:label "maintains quality for duplication of" .

# Classes
:ProductEngineeringDesignDataAndAssociatedLists rdf:type owl:Class ;
    rdfs:subClassOf iao:0000030 ; # iao:InformationContentEntity
    rdfs:label "Product Engineering Design Data and Associated Lists" ;
    rdfs:comment "PEDDAL as described in DI-SESS-81000F." ;
    # Axiom 1: Conformance to MIL-STD-31000
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty :conforms_to_standard ;
        owl:someValuesFrom :MIL_STD_31000_Doc
    ] ;
    # Axiom 2 (Part 1): Contains DesignDisclosureInformation
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iao:0000136 ; # iao:contains
        owl:someValuesFrom :DesignDisclosureInformation
    ] ;
    # Axiom 4 (Sub-requirement a): Reflects current design maturity
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty :reflects ;
        owl:someValuesFrom [
            rdf:type owl:Class ;
            owl:intersectionOf (
                :EndProduct
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty ro:0000086 ; # ro:has_quality
                    owl:someValuesFrom :CurrentDesignMaturityLevel
                ]
            )
        ]
    ] ;
    # Axiom 5 (Sub-requirement b): Provides data for logistics support products
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty :provides_engineering_data_for ;
        owl:someValuesFrom :LogisticsSupportProduct
    ] ;
    # Axiom 6 (Sub-requirement c - competitive acquisition):
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty :provides_data_permitting ;
        owl:someValuesFrom [
            rdf:type owl:Class ;
            owl:intersectionOf (
                :CompetitiveAcquisitionProcess
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty :concerns_item ;
                    owl:someValuesFrom :Item # The original item
                ]
            )
        ]
    ] ;
    # Axiom 6 (Sub-requirement c - manufacture of original item):
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty :provides_data_permitting ;
        owl:someValuesFrom [
            rdf:type owl:Class ;
            owl:intersectionOf (
                :ManufacturingProcess # Assuming this is for original item
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty ro:0002234 ; # ro:has_output
                    owl:someValuesFrom :Item # The original item
                ]
            )
        ]
    ] .

:StandardDocument rdf:type owl:Class ;
    rdfs:subClassOf iao:0000030 ; # iao:InformationContentEntity
    rdfs:label "Standard Document" .

:MIL_STD_31000_Doc rdf:type owl:NamedIndividual ,
                             :StandardDocument ;
    rdfs:label "MIL-STD-31000 Document" .

:DesignDisclosureInformation rdf:type owl:Class ;
    rdfs:subClassOf iao:0000030 ; # iao:InformationContentEntity
    rdfs:label "Design Disclosure Information" ;
    # Axiom 2 (Part 2): Serves as ClearCompleteAccurateTechnicalDescription
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty :serves_as ;
        owl:someValuesFrom :ClearCompleteAccurateTechnicalDescription
    ] .

:TechnicalDescription rdf:type owl:Class ;
    rdfs:subClassOf iao:0000030 ; # iao:InformationContentEntity ;
    rdfs:label "Technical Description" .

:ClearCompleteAccurateTechnicalDescription rdf:type owl:Class ;
    rdfs:subClassOf :TechnicalDescription ;
    rdfs:label "Clear, Complete, and Accurate Technical Description" ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty iao:0000136 ; # iao:is_about (assuming this is the relation intended for iao:is_about Item from your text)
        owl:someValuesFrom :Item
    ] ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty ro:0000086 ; # ro:has_quality
        owl:someValuesFrom :ClarityQuality
    ] ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty ro:0000086 ; # ro:has_quality
        owl:someValuesFrom :CompletenessQuality
    ] ;
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty ro:0000086 ; # ro:has_quality
        owl:someValuesFrom :AccuracyQuality
    ] ;
    # Axiom 3 (Part 1): Enables ProductionProcessForDuplication
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty ro:0002233 ; # ro:enables
        owl:someValuesFrom [
            rdf:type owl:Class ;
            owl:intersectionOf (
                :ProductionProcess
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty ro:0000057 ; # ro:agent_in
                    owl:someValuesFrom :ManufacturerWithSimilarCapability
                ]
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty ro:0002234 ; # ro:has_output
                    owl:someValuesFrom [
                        rdf:type owl:Class ;
                        owl:intersectionOf (
                            :Item
                            [
                                rdf:type owl:Restriction ;
                                owl:onProperty :duplicates_specified_characteristics_of ;
                                owl:someValuesFrom :Item # The original item
                            ]
                        )
                    ]
                ]
            )
        ]
    ] ;
    # Axiom 3 (Part 2): Enables QualityControlProcess
    rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty ro:0002233 ; # ro:enables
        owl:someValuesFrom [
            rdf:type owl:Class ;
            owl:intersectionOf (
                :QualityControlProcess
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty ro:0000057 ; # ro:agent_in
                    owl:someValuesFrom :ManufacturerWithSimilarCapability
                ]
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty :maintains_quality_for_duplication_of ;
                    owl:someValuesFrom :Item # The item whose duplication quality is maintained
                ]
            )
        ]
    ] .

:Item rdf:type owl:Class ;
    rdfs:subClassOf bfo:0000002 ; # bfo:MaterialEntity (assuming BFO for Item)
    rdfs:label "Item" .

:ManufacturerWithSimilarCapability rdf:type owl:Class ;
    rdfs:subClassOf org:Organization ;
    rdfs:label "Manufacturer With Similar Capability" .
    # Could add a restriction for 'has_capability :SimilarStateOfTheArtProduction' if :SimilarStateOfTheArtProduction is defined

:ProductionProcess rdf:type owl:Class ;
    rdfs:subClassOf bfo:0000015 ; # bfo:Process
    rdfs:label "Production Process" .

:QualityControlProcess rdf:type owl:Class ;
    rdfs:subClassOf bfo:0000015 ; # bfo:Process
    rdfs:label "Quality Control Process" .

# Qualities
:ClarityQuality rdf:type owl:Class ;
    rdfs:subClassOf bfo:0000019 ; # bfo:Quality
    rdfs:label "Clarity Quality" .

:CompletenessQuality rdf:type owl:Class ;
    rdfs:subClassOf bfo:0000019 ; # bfo:Quality
    rdfs:label "Completeness Quality" .

:AccuracyQuality rdf:type owl:Class ;
    rdfs:subClassOf bfo:0000019 ; # bfo:Quality
    rdfs:label "Accuracy Quality" .

:DesignMaturityLevel rdf:type owl:Class ;
    rdfs:subClassOf bfo:0000019 ; # bfo:Quality
    rdfs:label "Design Maturity Level" .

:CurrentDesignMaturityLevel rdf:type owl:NamedIndividual , # Assuming it's a specific level instance
                                     :DesignMaturityLevel ;
    rdfs:label "Current Design Maturity Level" .

:EndProduct rdf:type owl:Class ;
    rdfs:subClassOf :Item ;
    rdfs:label "End Product" .

:LogisticsSupportProduct rdf:type owl:Class ;
    rdfs:subClassOf :Item ;
    rdfs:label "Logistics Support Product" .

:CompetitiveAcquisitionProcess rdf:type owl:Class ;
    rdfs:subClassOf bfo:0000015 ; # bfo:Process
    rdfs:label "Competitive Acquisition Process" .

:ManufacturingProcess rdf:type owl:Class ;
    rdfs:subClassOf bfo:0000015 ; # bfo:Process
    rdfs:label "Manufacturing Process" .

